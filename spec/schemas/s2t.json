{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "word": {
        "type": "string"
      },
      "confidence": {
        "type": "number",
        "minimum": 0,
        "maximum": 1
      },
      "startTime": {
        "type": "number",
        "minimum": 0
      },
      "endTime": {
        "type": "number",
        "minimum": 0
      }
    },
    "required": ["word", "confidence", "startTime", "endTime"]
  }
}
